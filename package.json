{
  "name": "openai-translator",
  "version": "0.1.0",
  "description": "openai-translator",
  "main": "index.js",
  "scripts": {
    "build-tauri-renderer": "node build-tauri.mjs",
    "dev-tauri-renderer": "node build-tauri.mjs --serve",
    "build-tauri": "tauri build",
    "dev-tauri": "tauri dev",
    "dev-browser-extension": "chokidar src -c \"npm run build-browser-extension\"",
    "build-browser-extension": "make build",
    "clean": "make clean",
    "test": "npx jest",
    "lint": "eslint src/**/*.{ts,tsx} --cache",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc --cache"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yetone/openai-translator.git"
  },
  "dependencies": {
    "@sentry/react": "^7.49.0",
    "@tauri-apps/api": "^1.2.0",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-icons": "^3.0.0",
    "baseui-sd": "^12.2.4",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "dexie": "^3.2.3",
    "electron-store": "^8.1.0",
    "eventsource-parser": "^0.1.0",
    "hotkeys-js": "^3.10.1",
    "i18next": "^22.4.11",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "iso-639-1": "^2.1.15",
    "jss": "^10.10.0",
    "jss-preset-default": "^10.10.0",
    "lru-cache": "^8.0.0",
    "rc-field-form": "^1.27.4",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^3.1.4",
    "react-ga4": "^2.1.0",
    "react-hooks-global-state": "^2.1.0",
    "react-hot-toast": "^2.4.0",
    "react-hotkeys-hook": "^4.3.7",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.8.0",
    "react-jss": "^10.10.0",
    "react-markdown": "^8.0.7",
    "styletron-engine-atomic": "^1.5.0",
    "styletron-react": "^6.1.0",
    "tauri-plugin-autostart-api": "https://github.com/tauri-apps/tauri-plugin-autostart",
    "tesseract.js": "^4.0.2",
    "underscore": "^1.13.6",
    "url-join-ts": "^1.0.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@tauri-apps/cli": "^1.2.3",
    "@types/chrome": "0.0.158",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.14.6",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/tampermonkey": "^4.0.10",
    "@types/underscore": "^1.11.4",
    "@types/uuid": "^9.0.1",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "archiver": "^5.3.1",
    "base64-inline-loader": "^2.0.1",
    "chokidar-cli": "^3.0.0",
    "electron": "^23.1.3",
    "electron-util": "0.17.x",
    "esbuild": "^0.16.x",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-inline-import": "^1.0.1",
    "esbuild-server": "^0.1.2",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-baseui": "^12.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.1.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2",
    "webextension-polyfill": "^0.10.0"
  },
  "build": {
    "appId": "xyz.yetone.apps",
    "productName": "OpenAI Translator",
    "extraMetadata": {
      "name": "openai-translator",
      "main": "main.js"
    },
    "files": [
      {
        "from": ".",
        "filter": [
          "package.json"
        ]
      },
      {
        "from": "dist/electron/main"
      },
      {
        "from": "dist/electron/renderer"
      }
    ],
    "win": {
      "icon": "src/electron/assets/images/icon.png",
      "target": [
        "zip"
      ]
    },
    "mac": {
      "icon": "src/electron/assets/images/icon.png",
      "target": [
        "zip"
      ]
    },
    "linux": {
      "icon": "src/electron/assets/images/icon.png",
      "target": [
        "zip"
      ]
    },
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  }
}
